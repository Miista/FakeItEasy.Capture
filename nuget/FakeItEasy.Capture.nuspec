<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
	<id>FakeItEasy.Capture</id>
	<version>1.0.0-alpha</version>
	<title>FakeItEasy.Capture: Easily capturing arguments sent to any fake</title>
    <authors>Søren Guldmund</authors>
	<owners>Søren Guldmund</owners>
	<projectUrl>https://github.com/miista/FakeItEasy.Capture</projectUrl>
	<license type="expression">MIT</license>
	<requireLicenseAcceptance>false</requireLicenseAcceptance>
	<description>An AutoFixture relay that allows AutoFixture to create instances of the collections found in the System.Collections.Immutable namespace.</description>
	<copyright>Copyright 2020</copyright>
	<tags>FakeItEasy fake capture argument unit test</tags>

    <dependencies>
	  <group targetFramework="netstandard2.0">
	    <dependency id="AutoFixture" version="4.11.0" />
		<dependency id="System.Collections.Immutable" version="1.5.0" />
	  </group>
	</dependencies>
  </metadata>

  <files>
	<file src="netstandard2.0\*.dll" target="lib\netstandard2.0" />
	<file src="netstandard2.0\*.pdb" target="lib\netstandard2.0" />
	<file src="netstandard2.0\*.xml" target="lib\netstandard2.0" />
	<file src="net46\*.dll" target="lib\net46" />
	<file src="net46\*.xml" target="lib\net46" />
	<file src="net46\*.pdb" target="lib\net46" />
  </files>
</package>
